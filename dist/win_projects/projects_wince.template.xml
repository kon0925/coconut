  <preprocessor config="Debug">DEBUG</preprocessor>
  <preprocessor config="Release">NDEBUG</preprocessor>
  <preprocessor config="dll">_USRDLL</preprocessor>
  <preprocessor config="app">POCKETPC2003_UI_MODEL</preprocessor>
